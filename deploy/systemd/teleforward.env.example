# /etc/teleforward/teleforward.env
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
# Prefer setting this for headless VPS runs (or run `python main.py tui` once to save into DB)
TELEGRAM_SESSION_STRING=

# systemd StateDirectory=teleforward -> /var/lib/teleforward
DATABASE_PATH=/var/lib/teleforward/teleforward.db
DATA_DIR=/var/lib/teleforward
LOG_LEVEL=INFO
DISCORD_ALLOW_MASS_MENTIONS=false
DISCORD_SUPPRESS_URL_EMBEDS=true
DISCORD_STRIP_URLS=false
DISCORD_INCLUDE_TELEGRAM_LINK=true
